{
  "name": "qlobber-fsq",
  "description": "Shared file system queue. Supports pub-sub, work queues, wildcards, multi-process and distributed use.",
  "version": "9.0.0",
  "homepage": "https://github.com/davedoesdev/qlobber-fsq",
  "author": {
    "name": "David Halls",
    "email": "dave@davedoesdev.com",
    "url": "http://www.davedoesdev.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/davedoesdev/qlobber-fsq.git"
  },
  "bugs": {
    "url": "https://github.com/davedoesdev/qlobber-fsq/issues"
  },
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "test": "grunt lint test",
    "travis-test-4": "grunt lint test",
    "travis-test-6": "grunt lint test",
    "travis-test-8": "grunt lint && grunt coverage --getdents_size=1024 --disruptor --napi-modules --harmony-async-iteration && grunt coveralls",
    "travis-test-stress-4": "grunt test-stress",
    "travis-test-stress-6": "grunt test-stress",
    "travis-test-stress-8": "node --napi-modules ./node_modules/.bin/grunt test-stress --disruptor",
    "travis-test-multi-4": "grunt test-multi",
    "travis-test-multi-6": "grunt test-multi",
    "travis-test-multi-8": "node --napi-modules ./node_modules/.bin/grunt test-multi --disruptor",
    "travis-test-multi-remote-4": "grunt test-multi --remote=localhost",
    "travis-test-multi-remote-6": "grunt test-multi --remote=localhost",
    "travis-test-multi-remote-8": "grunt test-multi --remote=localhost",
    "bench-8": "grunt bench --rounds=1000 --size=1000 --ttl=60 --queues=10 --bucket_stamp_size=0 --ephemeral --disruptor --napi-modules"
  },
  "directories": {
    "test": "test",
    "lib": "lib"
  },
  "keywords": [
    "amqp",
    "mqtt",
    "rabbitmq",
    "cybertron",
    "ascoltatore",
    "qlobber",
    "pubsub",
    "queue",
    "routing"
  ],
  "dependencies": {
    "array-from": "~2.1.1",
    "async": "~2.5.0",
    "graceful-fs": "~4.1.11",
    "object.assign": "~4.0.4",
    "qlobber": "~1.8.0",
    "wu": "~2.1.0"
  },
  "optionalDependencies": {
    "fs-ext": "~0.6.0",
    "getdents": "0.0.1",
    "shared-memory-disruptor": "^1.1.1"
  },
  "devDependencies": {
    "grunt": "~1.0.1",
    "grunt-eslint": "~20.1.0",
    "grunt-mocha-test": "~0.13.2",
    "grunt-apidox": "~0.1.10",
    "grunt-exec": "~3.0.0",
    "mocha": "~3.5.3",
    "chai": "~4.1.2",
    "nyc": "~11.2.1",
    "coveralls": "~2.13.1",
    "b": "git://github.com/davedoesdev/b.git",
    "rimraf": "~2.6.2",
    "lsof": "~0.1.0",
    "yargs": "~9.0.1",
    "cp-remote": "~0.1.3",
    "babel-eslint": "^8.0.0"
  },
  "engines": {
    "node": ">=4"
  }
}
